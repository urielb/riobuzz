meta(charset="utf-8")
meta(content="IE=edge,chrome=1", http-equiv="X-UA-Compatible")
meta(name="viewport", content="width=device-width, minimal-ui, initial-scale=1, maximum-scale=1.0, user-scalable=no")
    
each val in [144, 114, 72, 57]
    link(rel="apple-touch-icon-precomposed", sizes=val+"x"+val, href="images/apple-touch-icon-"+val+"-precomposed.png")
    
title= title

script(type='text/javascript').
    var polyfilter_scriptpath = "/js/polyfills/filters/";
    window.polyfilter_skip_stylesheets = true;
    
link(rel='stylesheet', href='/css/bundle.css')